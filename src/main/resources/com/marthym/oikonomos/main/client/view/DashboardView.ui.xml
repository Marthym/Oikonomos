<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:nomos="urn:import:com.marthym.oikonomos.main.client.components">
	
	<ui:with type="com.marthym.oikonomos.main.client.resources.DashboardViewResource" field="res" />
	
	<g:DockLayoutPanel ui:field="dockPanel">
		<g:north size="42">
			<g:HTMLPanel height="36" ui:field="topPanel" />
		</g:north>
		<g:west size="300">
			<g:HTMLPanel width="300" ui:field="leftPanel" />
		</g:west>
		<g:center>
			<g:HTMLPanel styleName="{res.style.content}" ui:field="centerPanel" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 